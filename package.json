  "scripts": {
    "test": "grunt test",
    "build:clean": "rm -rf dist bower-dist app/generated .tmp",
    "build:mkdist": "mkdir dist; mkdir -p dist/$npm_package_name-$npm_package_version/styles; (cd dist; ln -s $npm_package_name-$npm_package_version latest)",
    "build:css:lforms": "cleancss --skip-rebase -o dist/latest/styles/lforms.min.css .tmp/styles/main.css .tmp/styles/lforms.css bower_components/jquery-ui/themes/start/jquery-ui.min.css",
    "build:css:themes": "node-sass app/styles/themes.scss --output .tmp/styles && cleancss --skip-rebase -o dist/latest/styles/themes.min.css .tmp/styles/themes.css",
    "build:css": "npm run build:css:themes && npm run build:css:lforms",
    "build:js": "uglifyjs .tmp/lforms.js bower_components/jquery/dist/jquery.js bower_components/jquery-ui/ui/core.js bower_components/jquery-ui/ui/widget.js bower_components/jquery-ui/ui/button.js bower_components/jquery-ui/ui/dialog.js bower_components/jquery-ui/ui/datepicker.js --source-map \"url='lforms.min.js.map',includeSources\" --output dist/latest/lforms.min.js",
    "build": "npm run build:clean && npm run build:mkdist && webpack && npm run build:js && npm run build:css",
    "update-webdriver": "node node_modules/protractor/bin/webdriver-manager update"
  }
}
